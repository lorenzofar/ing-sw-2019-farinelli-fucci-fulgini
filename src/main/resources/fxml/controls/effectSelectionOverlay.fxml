<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:id="overlayRoot"
         styleClass="effect-overlay"
         stylesheets="@../style.css"
         type="javafx.scene.control.Button">

    <GridPane fx:id="effectContent" vgap="8">
        <Label styleClass="title-text" fx:id="effectName" GridPane.rowIndex="0"/>
        <HBox fx:id="costContainer" spacing="8" GridPane.rowIndex="1"/>
        <Label fx:id="effectDescription" GridPane.rowIndex="2"/>
    </GridPane>

</fx:root>
